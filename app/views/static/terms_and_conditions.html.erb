  <button onclick="topFunction()" id="myBtn" title="Go to top" class="btn btn-primary position-fixed bottom-0 end-0 mb-3 me-3"><i class="fas fa-arrow-up"></i></button>
<script>
document.addEventListener('DOMContentLoaded', (event) => {
  let toggleButtons = document.querySelectorAll('.toggleButton');
  toggleButtons.forEach(function(toggleButton) {
    let divId = toggleButton.getAttribute('data-toggle');
    let div = document.getElementById(divId);
    let icon = toggleButton.querySelector('i');

    // Initialize display style and icon
    div.style.display = "none";
    icon.classList.remove('fa-eye');
    icon.classList.add('fa-eye-slash');

    toggleButton.addEventListener('click', function(e) {
      event.stopPropagation();
      if (div.style.display === "none") {
        div.style.display = "block";
        icon.classList.remove('fa-eye-slash');
        icon.classList.add('fa-eye');
      } else {
        div.style.display = "none";
        icon.classList.remove('fa-eye');
        icon.classList.add('fa-eye-slash');
      }
    });
  });

  let mybutton = document.getElementById("myBtn");
  window.addEventListener('scroll', function() {
    if (document.body.scrollTop > 20 || document.documentElement.scrollTop > 20) {
      mybutton.style.display = "block";
    } else {
      mybutton.style.display = "none";
    }
  });

  mybutton.addEventListener('click', function() {
    document.body.scrollTop = 0;
    document.documentElement.scrollTop = 0;
  });
});
let links = document.querySelectorAll('h3 a');
links.forEach(function(link) {
  link.addEventListener('click', function(e) {
    e.preventDefault();
    let href = link.getAttribute('href');
    let target = document.querySelector(href);
    target.scrollIntoView({behavior: "smooth"});
  });
});
</script>
