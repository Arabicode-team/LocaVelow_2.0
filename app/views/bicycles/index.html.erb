<p style="color: green"><%= notice %></p>
<%= render 'shared/banner' %>
<h1>Bicycles</h1>

<div id="index-map" style="height: 400px;"></div>

<div id="bicycles">
  <% @bicycles.each do |bicycle| %>
    <%= render bicycle %>
    <p>
      <%= link_to "Show this bicycle", bicycle %>
    </p>
  <% end %>
</div>

<%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=#{ENV['GOOGLE_MAPS_API_KEY']}&callback=initIndexMap", defer: true %>
<%= javascript_include_tag 'indexMap' %>
