
<h1>Accessories</h1>

<div id="accessories">
  <% @accessories.each do |accessory| %>
    <%= render accessory %>
    <p>
      <%= link_to "Show this accessory", accessory, aria_label: "Show accessory" %>
    </p>
  <% end %>
</div>

<%= link_to "New accessory", new_accessory_path, aria_label: "New accessory" %>
